{
  "name": "tasmota-temperature-gauge",
  "productName": "tempReader",
  "private": true,
  "version": "0.0.1",
  "description": "Sonoff Th10 Tasmota Temp Reader",
  "main": "Electron/app.js",
  "module": "build/index.mjs",
  "es2015": "build/esm/index.mjs",
  "es2017": "build/esm/index.mjs",
  "jsnext:main": "build/esm/index.mjs",
  "scripts": {
    "build": "stencil build --prerender",
    "dev": "stencil build --dev --watch --serve",
    "devWithSW": "sd concurrent \"stencil build --dev --service-worker --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache",
    "packager": "npm run build --prod && npx tsc ./electron/createInstaller.ts && npx tsc ./electron/createInstaller.ts && npx tsc ./electron/app.ts && npx electron-packager . --out ../tray-sonoff-temp-package --platform=win32 --overwrite"
  },
  "dependencies": {
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@ionic/core": "^5.0.5",
    "chart.js": "^2.8.0",
    "d3": "^5.15.0",
    "flux-standard-action": "^2.1.1",
    "localforage": "^1.7.3",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "push.js": "^1.0.12",
    "react": "^16.12.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.5",
    "redux-form": "^8.2.6",
    "redux-orm": "^0.13.3",
    "rxjs": "^6.5.4",
    "electron-serve": "^0.4.1",
    "electron-store": "^5.1.1",
    "node-notifier": "^6.0.0"
  },
  "devDependencies": {
    "@stencil/core": "^1.9.2",
    "@stencil/dev-server": "latest",
    "@stencil/redux": "^0.1.2",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.1.1",
    "@stencil/utils": "latest",
    "@types/chart.js": "^2.8.7",
    "@types/chartist": "^0.9.46",
    "@types/d3": "^5.7.2",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/redux": "^3.6.0",
    "@types/redux-orm": "^0.13.6",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "electron-forge": "^5.2.4",
    "electron-packager": "^14.2.1",
    "electron-wix-msi": "^2.2.0",
    "jest": "^24.9.0",
    "node-gyp": "^6.1.0",
    "shelljs": "^0.8.3",
    "ts-node": "^8.8.1",
    "tslint": "^5.20.1",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "^3.7.5",
    "workbox-build": "4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-app-starter.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil"
  },
  "homepage": "https://github.com/ionic-team/stencil",
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "build": {
    "productName": "MyApp",
    "compression": "store",
    "forceCodeSigning": false,
    "asar": false,
    "win": {
      "icon": "/src/assets/icon/thermometer.ico"
    }
  }
}
